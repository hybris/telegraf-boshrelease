check process telegraf
  with pidfile /var/vcap/sys/run/telegraf/telegraf.pid
  start program "/var/vcap/jobs/telegraf/bin/monit_debugger telegraf_ctl '/var/vcap/jobs/telegraf/bin/ctl start'" with timeout 60 seconds
  stop program "/var/vcap/jobs/telegraf/bin/monit_debugger telegraf_ctl '/var/vcap/jobs/telegraf/bin/ctl stop'" with timeout 60 seconds
  group vcap
